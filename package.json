    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/progress": "^2.0.7",
    "@types/semver": "^7.5.8",
    "@types/tar": "^6.1.13",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.56.0",
    "globals": "^15.4.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  }
