{
  "name": "@brkncookie/pm_junior",
  "version": "1.0.4",
  "description": "a basic nodeJS package-manager",
  "main": "dist/index.js",
  "bin": {
    "pm_junior": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "link": "npm run build && npm link && sed -i '1s|^|#!/usr/bin/env node\\n|' dist/index.js",
